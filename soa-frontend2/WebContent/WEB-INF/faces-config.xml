<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <managed-bean>
  <managed-bean-name>userManager</managed-bean-name>
  <managed-bean-class>authorization.UserManager</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>password</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>username</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>accountCreator</managed-bean-name>
  <managed-bean-class>account.AccountCreator</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>email</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>password</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>phoneNumber</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>name</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>surname</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>username</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>tableAllConferences</managed-bean-name>
  <managed-bean-class>userhome.TableAllConferences</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>columns</property-name>
   <property-class>java.util.List</property-class>
   <list-entries/>
  </managed-property>
  <managed-property>
   <property-name>conferences</property-name>
   <property-class>java.util.List</property-class>
   <list-entries/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>conferenceCreator</managed-bean-name>
  <managed-bean-class>conference.ConferenceCreator</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>name</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>city</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>date</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>description</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>speaker</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>bio</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/pages/authorization/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>userhome</from-outcome>
   <to-view-id>/pages/userhome/userhome.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>create-account</from-outcome>
   <to-view-id>/pages/authorization/create-account.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/authorization/create-account.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>/pages/authorization/login.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/userhome/userhome.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>/pages/authorization/login.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>conferences-all</from-outcome>
   <to-view-id>/pages/userhome/conferences-all.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>create-conference</from-outcome>
   <to-view-id>/pages/userhome/create-conference.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/userhome/conferences-all.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>userhome</from-outcome>
   <to-view-id>/pages/userhome/userhome.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/userhome/create-conference.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>userhome</from-outcome>
   <to-view-id>/pages/userhome/userhome.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
</faces-config>

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="model">
 <class name="Conference" table="_conference">
  <id column="id" name="id">
   <generator class="increment"/>
  </id>

  <property column="name" name="name" type="java.lang.String" unique="true"/>
  <property column="city" name="city" type="java.lang.String"/>
  <property column="date" name="date" type="java.lang.String"/>
  <property column="description" name="description" type="java.lang.String"/>
  <property column="speaker" name="speaker" type="java.lang.String"/>
  <property column="bio" name="bio" type="java.lang.String"/>
  <property column="lat" name="lat" type="java.lang.Double"/>
  <property column="lon" name="lon" type="java.lang.Double"/>
  
 </class>
</hibernate-mapping>
